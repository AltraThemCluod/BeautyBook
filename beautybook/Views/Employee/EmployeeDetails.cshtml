                                        </div>
                                    </div>*@

                                </div>
                            </div>

                            <hr>

                            <div class="card-body pb-0">
                                <div class="row">

                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label for="selectCountry" class="col-sm-4 col-form-label text-sm-right">@(beautybook.Resources.Languages.Country)</label>

                                            <div class="col-sm-7">
                                                <select class="selectpicker form-control" id="employeeCountry" tabindex="13" name="employeeCountry" title="@(beautybook.Resources.Languages.Select_a_Country)" data-live-search="true" data-size="6"></select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label for="selectState" class="col-sm-4 col-form-label text-sm-right">@(beautybook.Resources.Languages.State)</label>

                                            <div class="col-sm-7">
                                                <select class="selectpicker form-control" id="employeeState" tabindex="14" name="employeeState" title="@(beautybook.Resources.Languages.Select_a_State)" data-live-search="true" data-size="6" disabled></select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label for="selectCity" class="col-sm-4 col-form-label text-sm-right">@(beautybook.Resources.Languages.City)</label>
                                            <div class="col-sm-7">
                                                <input type="text" class="form-control" id="employeeCity" name="employeeCity" tabindex="15" placeholder="@(beautybook.Resources.Languages.Enter_your_city)">
                                            </div>
                                        </div>
                                    </div>


                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label for="addressLine1" class="col-sm-4 col-form-label text-sm-right">@(beautybook.Resources.Languages.Address_line_1)</label>

                                            <div class="col-sm-7">
                                                <input type="text" class="form-control" id="employeeAddressLine1" name="employeeAddressLine1" tabindex="16" placeholder="Box No. 65438, Mishui Avenue">
                                            </div>
                                        </div>
                                    </div>
                                    <input type="hidden" class="form-control" id="VisitorLatitude" name="VisitorLatitude" autocomplete="off" />
                                    <input type="hidden" class="form-control" id="VisitorLongitude" name="VisitorLongitude" autocomplete="off" />

                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label for="addressLine2" class="col-sm-4 col-form-label text-sm-right">@(beautybook.Resources.Languages.Address_line_2)</label>

                                            <div class="col-sm-7">
                                                <input type="text" class="form-control" id="employeeAddressLine2" name="employeeAddressLine2" tabindex="17" placeholder="@(beautybook.Resources.Languages.Gaomi_City)">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label for="zipCode" class="col-sm-4 col-form-label text-sm-right">@(beautybook.Resources.Languages.Zip_Code)</label>

                                            <div class="col-sm-7">
                                                <input type="text" class="form-control" id="employeeZipcode" name="employeeZipcode" tabindex="18" placeholder="@(beautybook.Resources.Languages.Enter_your_zipcode)">
