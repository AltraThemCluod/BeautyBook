                        </div>

                        <div class="card card-lg mb-3" id="workingHourse" style="display:none;">
                            <div class="card-header">
                                <h6 class="mb-1">@(beautybook.Resources.Languages.Working_Hours)</h6>
                                <p class="mb-0 text-muted fs-13">@(beautybook.Resources.Languages.Tell_us_all_about_your_new_employee)</p>
                            </div>

                            <div class="card-body">
                                <div class="row">
                                    <div class="col-sm-7">
                                        <div class="table-responsive table-card">
                                            <table class="table table-sm fs-14 table-hover">
                                                <thead>
                                                    <tr>
                                                        <th class="font-weight-semibold" scope="col">@(beautybook.Resources.Languages.Day)</th>
                                                        <th class="font-weight-semibold" scope="col">@(beautybook.Resources.Languages.START_AT)</th>
                                                        <th class="font-weight-semibold" scope="col">End at</th>
                                                    </tr>
                                                </thead>
                                                <tbody id="userWorkinghourslist">
                                                    @*User working hours list bind*@
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="card card-lg mb-3" id="srvPrcconfiguration" style="display:none;">
                            <div class="card-header">
                                <h6 class="mb-1">Service & Pricing Configuration</h6>
                                <p class="mb-0 text-muted fs-13">@(beautybook.Resources.Languages.Tell_us_all_about_your_new_employee)</p>
                            </div>

                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="table-responsive table-card">
                                            <table class="table fs-13">
                                                <thead>
                                                    <tr>
                                                        <th scope="col" class="bg-primary text-white w-lg-60">@(beautybook.Resources.Languages.HEALTH_CARE)</th>
                                                        <th scope="col" class="bg-primary text-white">@(beautybook.Resources.Languages.Duration)</th>
                                                        <th scope="col" class="bg-primary text-white">@(beautybook.Resources.Languages.Price)</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <!-- New Category -->
                                                    <tr>
                                                        <th scope="row" class="bg-gray-100" colspan="3">
                                                            <div class="custom-control custom-checkbox">
                                                                <input type="checkbox" class="custom-control-input selectall" id="hairSalon" data-group="1">
                                                                <label class="custom-control-label" for="hairSalon">@(beautybook.Resources.Languages.Hair_Salon)</label>
                                                            </div>
                                                        </th>
                                                    </tr>

                                                    <!-- Service Name -->
