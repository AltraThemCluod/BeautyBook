    function addEmployee(Createnewemployee) {

        $("form[name='employeeForm']").validate({
            rules: {
                employeeFirstname: {
                    required: true
                },
                employeeLastname: {
                    required: true
                },
                employeeDob: {
                    required: true
                },
                employeeEmail: {
                    required: true,
                    email: true
                },
                employeePrimaryphone: {
                    required: true,
                    number: true,
                    maxlength: 9,
                    minlength: 9
                },
                employeeAlternatephone: {
                    number: true,
                    maxlength: 9,
                    minlength: 9
                },
                employeeJoiningdate: {
                    required: true
                },
                employeeCity: {
                    required: true
                },
                employeeAddressLine1: {
                    required: true
                },
                employeeZipcode: {
                    required: true,
                    number: true
                }
            },
            messages: {
                employeeFirstname: "@beautybook.Resources.Languages.First_Name_is_required",
                employeeLastname: "@beautybook.Resources.Languages.Last_Name_is_required",
                employeeDob: "@beautybook.Resources.Languages.Date_of_Birth_is_required",
                employeeEmail: {
                    required: "@beautybook.Resources.Languages.Email_is_required",
                },
                employeePrimaryphone: {
                    required: "@beautybook.Resources.Languages.Primary_Phone_is_required",
                    number : "@beautybook.Resources.Languages.Please_enter_a_valid_number"
                },
                employeeAlternatephone: {
                    number : "@beautybook.Resources.Languages.Please_enter_a_valid_number"
                },
                employeeJoiningdate: "@beautybook.Resources.Languages.Joining_date_is_required",
                employeeCity: "@beautybook.Resources.Languages.City_is_required",
                employeeAddressLine1: "@beautybook.Resources.Languages.Address_Line1_is_required",
                employeeZipcode: {
                    required : "@beautybook.Resources.Languages.Zipcode_is_required",
                },
